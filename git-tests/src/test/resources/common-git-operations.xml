<!--Each test from this suite will run twice: with jgit first, and then with native git enabled.
    So only tests that perform some git operations should be placed in this suite -->

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Git Suite">
  <test name="Git Tests">
    <classes>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.GitVcsSupportTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.TestConnectionTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.SubmoduleTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.CleanerTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.GitUrlSupportTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.ListFilesTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.MapFullPathTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.GitMergeSupportTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.GitCommitSupportTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.GitLabelingSupportTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.GitPatchTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.GitRepositoryInitializingExtensionTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.ContentProviderTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.CollectChangesTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.LatestAcceptedRevisionTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.LatestAcceptedRevisionAndSubmodulesTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.GitRemoteUrlInspectorTest"/>
      <class name="jetbrains.buildServer.buildTriggers.vcs.git.tests.health.GitLocalFileUrlHealthReportTest"/>
    </classes>
  </test>
</suite>